# ~/.bash_aliases
alias a='git add -A && git commit'
alias b='bundle exec'
alias c='bundle exec rails console'
alias d='git diff'
alias g='git status'
alias h='history'
alias i='bundle install'
alias j='jobs'
alias l='ls -lh'
alias n='sudo netstat -ntlp'
alias p='git push'
alias r='bundle exec rake'
alias s='bundle exec rails server'
alias t='bundle exec rake test'
alias u='sudo aptitude safe-upgrade'
alias v='vim'
alias w='workspace-status'
alias y='workspace-sync'
